ECT 
    'sqlite' AS dbms,
    NULL AS TABLE_CATALOG,
    NULL AS TABLE_SCHEMA,
    m.name AS TABLE_NAME,
    p.name AS COLUMN_NAME,
    p.cid AS ORDINAL_POSITION,
    p.type AS DATA_TYPE,
    NULL AS CHARACTER_MAXIMUM_LENGTH,
    CASE 
        WHEN pk.name IS NOT NULL THEN 'PRIMARY KEY'
        WHEN fk.name IS NOT NULL THEN 'FOREIGN KEY'
        ELSE NULL
    END AS CONSTRAINT_TYPE,
    fk.table AS REFERENCED_TABLE_SCHEMA,
    fk.to AS REFERENCED_TABLE_NAME,
    fk.from AS REFERENCED_COLUMN_NAME
FROM 
    sqlite_master m
    LEFT JOIN pragma_table_info(m.name) p ON m.name = p.name
    LEFT JOIN (
        SELECT 
            m.name AS name,
            p.name AS name,
            p.cid AS cid
        FROM 
            sqlite_master m
            JOIN pragma_table_info(m.name) p ON m.name = p.name
        WHERE 
            p.pk = 1
    ) pk ON m.name = pk.name AND p.cid = pk.cid
    LEFT JOIN pragma_foreign_key_list(m.name) fk ON p.name = fk.from
WHERE 
    m.type = 'table';</sql><current_tab id="0"/></tab_sql></sqlb_project>
